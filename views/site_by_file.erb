<h2 style="text-align:center"><a href="<%= (Date.parse(@date_toi.to_s) - 1).to_s.gsub(/-/, '') %>">&lt;</a><%= @date %><a href="<%= (Date.parse(@date_toi.to_s) + 1).to_s.gsub(/-/, '') %>">&gt;</a></h2>
<table class="table table-striped table-bordered">
  <tr>
    <th>Rank</th>
    <th>Path</th>
    <th>Count</th>
  </tr>
<% @top.each_with_index do |l,index| %>
  <tr>
    <td><%= index + 1 %></td>
    <td><a href="<%= @date_toi %>/<%= l[0][1..-1] %>"><%= l[0] %></a></td>
    <td><%= l[1] %></td>
  </tr>
<% end %>
</table>
